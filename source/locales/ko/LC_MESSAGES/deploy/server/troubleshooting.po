#: ../../source/deploy/server/troubleshooting.rst:280
msgid "Customer name"
msgstr ""

#: ../../source/deploy/server/troubleshooting.rst:281
msgid "Customer contacts"
msgstr ""

#: ../../source/deploy/server/troubleshooting.rst:282
msgid "Customer license, e.g. Enterprise/Professional"
